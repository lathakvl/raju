	  Select CurrentLocationSelection=new Select(CurrentLocation);
      CurrentLocationSelection.deselectByVisibleText("");
      By industryLocator=By.id("");
      WebElement industry=driver.findElement(industryLocator);
      Select industrySelection=new Select();
      industrySelection.Select ByVisibleText)(other);
      industrySelection.SelectByvalue("");
      industrySelection.selectByIndex();
